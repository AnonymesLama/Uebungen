#Bogen in Gradmaß
x=float(input('Radiant: '))
from math import pi
deg = (180 * x) / pi
grad = int(deg)
bogenmin = x*10800/pi
bogensek = x*648000/pi
print(f'{deg}°')                  #Das umgerechnete Bogenmaß in Grad asugeben
print(f'{bogenmin} Bogenminuten') #Die ausgerechneten Bogenminuten ausgeben
print(f'{bogensek} Bogensekunden')#Die ausgerechneten Bogensekunden ausgeben
